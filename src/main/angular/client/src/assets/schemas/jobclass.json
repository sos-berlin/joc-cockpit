{
  "title": "jobClass",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "calendar",
  "type": "object",
  "required": [
    "path"
  ],
  "propertyOrder": [
    "TYPE",
    "path",
    "lifetime",
    "orderId"
  ],
  "description": "deploy object with fixed property 'TYPE':'JobClass'",
  "properties": {
    "path": {
      "description": "absolute path of a JobScheduler object.",
      "type": "string",
      "pattern": "^(/|(/[^/\\<>?:\"|*]+)+)$",
      "maxLength": 255,
      "minLength": 1
    },
    "maxProcesses": {
      "type": "integer",
      "minimum": 0
    },
    "orderId": {
      "type": "string",
      "pattern": "^[^<>]*$",
      "maxLength": 255
    },
    "priority": {
      "type": "string",
      "enum": [
        "idle",
        "below normal",
        "normal",
        "above normal",
        "high"
      ]
    }
  },
  "additionalProperties": false
}
