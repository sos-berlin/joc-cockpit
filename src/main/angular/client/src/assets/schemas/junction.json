{
  "title": "junction",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "calendar",
  "type": "object",
  "required": [
    "path"
  ],
  "propertyOrder": [
    "TYPE",
    "path",
    "lifetime",
    "orderId"
  ],
  "description": "deploy object with fixed property 'TYPE':'Junction'",
  "properties": {
    "path": {
      "description": "absolute path of a JobScheduler object.",
      "type": "string",
      "pattern": "^(/|(/[^/\\<>?:\"|*]+)+)$",
      "maxLength": 255,
      "minLength": 1
    },
    "lifetime": {
      "type": "integer",
      "minimum": 0
    },
    "orderId": {
      "type": "string",
      "pattern": "^[^<>]*$",
      "maxLength": 255
    },
    "title": {
      "type": "string",
      "pattern": "^[^<>]*$",
      "maxLength": 255
    }
  },
  "additionalProperties": false
}
