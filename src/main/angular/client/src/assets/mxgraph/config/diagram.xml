<mxEditor defaultGroup="group" defaultEdge="connector">
  <ui>
    <add as="graph" element="clusterStatus"/>
  </ui>
  <Array as="templates">
    <add as="connector">
      <Connector label="" type="">
        <mxCell edge="1">
          <mxGeometry as="geometry" relative="1"/>
        </mxCell>
      </Connector>
    </add>
    <add as="Job">
      <Job jobName="job">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="180" height="40"/>
        </mxCell>
      </Job>
    </add>
    <add as="If">
      <If label="if" predicate="$returnCode > 0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </If>
    </add>
    <add as="AddOrder">
      <AddOrder label="addOrder">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </AddOrder>
    </add>
    <add as="Fork">
      <Fork label="fork">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fork>
    </add>
    <add as="ForkList">
      <ForkList label="forkList">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ForkList>
    </add>
    <add as="Retry">
      <Retry label="retry" maxTries="10" retryDelays="0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Retry>
    </add>
    <add as="Lock" >
      <Lock label="lock">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Lock>
    </add>
    <add as="Cycle" >
      <Cycle label="cycle">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Cycle>
    </add>
    <add as="Try">
      <Try label="try">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Try>
    </add>
    <add as="Fail">
      <Fail label="fail">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fail>
    </add>
    <add as="Finish">
      <Finish label="finish">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Finish>
    </add>
    <add as="ExpectNotice" >
      <ExpectNotice label="expectNotice">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ExpectNotice>
    </add>
    <add as="PostNotice">
      <PostNotice label="postNotice">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </PostNotice>
    </add>
    <add as="Prompt">
      <Prompt label="prompt">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Prompt>
    </add>
    <add as="FileWatcher">
      <FileWatcher label="fileWatcher" regex=".*">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="110" height="40"/>
        </mxCell>
      </FileWatcher>
    </add>
    <add as="symbol">
      <Symbol label="Instruction">
        <mxCell vertex="1" style="symbol;image=images/symbols/event.png">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Symbol>
    </add>
  </Array>
  <mxGraph as="graph" alternateEdgeStyle="verticalConnector" allowLoops="1" dropEnabled="1">
    <add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
    <add as="isSwimlane"><![CDATA[
			function (cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'container');
			}
		]]></add>
    <mxStylesheet as="stylesheet">
      <add as="text">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="shadow" value="0"/>
        <add as="strokeWidth" value="0"/>
        <add as="strokeColor" value="none"/>
        <add as="fillColor" value="transparent"/>
        <add as="fillColor" value="#f5f7fb"/>
        <add as="gradientColor" value="white"/>
      </add>
      <add as="defaultVertex" extend="text">
        <add as="shape" value="rectangle"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="none"/>
        <add as="fillColor" value="#f5f7fb"/>
        <add as="strokeWidth" value="0"/>
      </add>
      <add as="defaultEdge">
        <add as="shape" value="connector"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="center"/>
        <add as="rounded" value="0"/>
        <add as="strokeColor" value="gray"/>
        <add as="edgeStyle" value="orthogonalEdgeStyle"/>
        <add as="endArrow" value="classic"/>
      </add>
      <add as="controller">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
      </add>
      <add as="circle">
        <add as="shape" value="ellipse"/>
        <add as="align" value="center"/>
      </add>
    </mxStylesheet>
    <mxGraphModel as="model">
      <root>
        <mxCell id="0"/>
        <mxCell parent="0"/>
      </root>
    </mxGraphModel>
  </mxGraph>
</mxEditor>
