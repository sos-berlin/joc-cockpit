<mxEditor defaultGroup="group" defaultEdge="connector">
  <include name="./assets/mxgraph/config/editor-commons.xml"/>
  <mxGraph as="graph" alternateEdgeStyle="verticalEdge" swimlaneNesting="0" dropEnabled="1">
    <add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
    <add as="isSwimlane"><![CDATA[
			function (cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'container');
			}
		]]></add>
    <mxStylesheet as="stylesheet">
      <add as="text">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="shadow" value="0"/>
        <add as="strokeColor" value="none"/>
        <add as="fillColor" value="transparent"/>
        <add as="gradientColor" value="none"/>
      </add>
      <add as="defaultVertex" extend="text">
        <add as="shape" value="rectangle"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="#333333"/>
      </add>
      <add as="group">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="gray"/>
        <add as="dashed" value="1"/>
      </add>
      <add as="defaultEdge">
        <add as="shape" value="connector"/>
        <add as="fontSize" value="10"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="center"/>
        <add as="rounded" value="1"/>
        <add as="labelBackgroundColor" value="#fff"/>
        <add as="strokeColor" value="gray"/>
        <add as="edgeStyle" value="elbowEdgeStyle"/>
        <add as="endArrow" value="classic"/>
      </add>
      <add as="swimlane">
        <add as="shape" value="swimlane"/>
        <add as="shadow" value="0"/>
        <add as="startSize" value="23"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="spacingTop" value="8"/>
				<add as="fontColor" value="white"/>
				<add as="separatorColor" value="#c0c0c0"/>
      </add>
      <add as="verticalEdge">
				<add as="elbow" value="vertical"/>
			</add>
      <add as="job">
        <add as="rounded" value="1"/>
        <add as="shape" value="label"/>
        <add as="image" value="./assets/mxgraph/images/symbols/job.svg"/>
        <add as="imageWidth" value="20"/>
        <add as="imageHeight" value="20"/>
	      <add as="strokeColor" value="#90C7F5"/>
				<add as="fillColor" value="#90C7F5"/>
        <add as="gradientColor" value="white"/>
        <add as="whiteSpace" value="wrap"/>
      </add>
      <add as="rectangle" extend="rhombus">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="align" value="center"/>
			</add>
      <add as="dashRectangle" extend="rhombus">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="align" value="center"/>
				<add as="dashed" value="1"/>
				<add as="shadow" value="0"/>
				<add as="opacity" value="70"/>
        <add as="strokeColor" value="#FFCF8c"/>
				<add as="fillColor" value="#FFCF8c"/>
        <add as="gradientColor" value="white"/>
			</add>
      <add as="catch" extend="rhombus">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="align" value="center"/>
        <add as="strokeColor" value="#FFCF8c"/>
				<add as="fillColor" value="#FFCF8c"/>
        <add as="gradientColor" value="white"/>
			</add>
      <add as="ellipse">
        <add as="shape" value="ellipse"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="fillColor" value="transparent"/>
      </add>
      <add as="order">
        <add as="shape" value="ellipse"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="fillColor" value="#ffc04c"/>
        <add as="strokeColor" value="#ffc04c"/>
        <add as="gradientColor" value="white"/>
        <add as="rounded" value="1"/>
      </add>
      <add as="if">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
        <add as="strokeColor" value="#CDEB8B"/>
				<add as="fillColor" value="#CDEB8B"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="white"/>
      </add>
      <add as="retry">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
	      <add as="strokeColor" value="#FFC7C7"/>
				<add as="fillColor" value="#FFC7C7"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="white"/>
      </add>
      <add as="try">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
        <add as="strokeColor" value="#FFCF8A"/>
				<add as="fillColor" value="#FFCF8A"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="white"/>
      </add>
      <add as="fileWatcher">
        <add as="shape" value="ellipse"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="strokeColor" value="#8B8BB4"/>
				<add as="fillColor" value="#8B8BB4"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="white"/>
      </add>
      <add as="symbol">
        <add as="shape" value="image"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="labelBackgroundColor" value="transparent"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="verticalLabelPosition" value="bottom"/>
      </add>
    </mxStylesheet>
    <mxGraphModel as="model">
      <root>
        <mxCell id="0"/>
        <mxCell parent="0"/>
      </root>
    </mxGraphModel>
  </mxGraph>
  <mxDefaultToolbar as="toolbar">
    <add as="connect" mode="connect" icon="./assets/mxgraph/images/symbols/connect.gif"/>
<!--    <add as="show" action="show" icon="./assets/mxgraph/images/preview.png"/>
    <hr/>-->

    <add as="Job" template="Job" style="job" icon="./assets/mxgraph/images/job.png"/>
    <hr/>

    <add as="Fork" template="Fork" style="symbol;image=./assets/mxgraph/images/symbols/fork.svg"
         icon="./assets/mxgraph/images/fork.png"/>
    <hr/>

    <add as="Retry" template="Retry" style="retry" icon="./assets/mxgraph/images/retry.png"/>
    <hr/>
    <add as="Try" template="Try" style="try" icon="./assets/mxgraph/images/try.png"/>
    <hr/>
    <add as="If" template="If" style="if" icon="./assets/mxgraph/images/if.png"/>
    <hr/>
    <add as="Finish" template="Finish" style="symbol;image=./assets/mxgraph/images/symbols/finish.svg"
         icon="./assets/mxgraph/images/finish.png"/>
    <hr/>
    <add as="Fail" template="Fail" style="symbol;image=./assets/mxgraph/images/symbols/fail.svg"
         icon="./assets/mxgraph/images/fail.png"/>
    <hr/>
    <add as="Publish" template="Publish" style="symbol;image=./assets/mxgraph/images/symbols/publish.svg" icon="./assets/mxgraph/images/publish.png"/>
    <hr/>
    <add as="Await" template="Await" style="symbol;image=./assets/mxgraph/images/symbols/await.svg"
         icon="./assets/mxgraph/images/await.png"/>
    <hr/>
    <add as="Lock" template="Lock" style="symbol;image=./assets/mxgraph/images/symbols/lock.svg"
         icon="./assets/mxgraph/images/lock.png"/>
    <hr/>
    <add as="FileWatcher" template="FileWatcher" style="fileWatcher" icon="./assets/mxgraph/images/fileWatcher.png"/>
    <hr/>
    <add as="Paste" template="symbol" style="symbol" icon="./assets/mxgraph/images/paste.png"/>
  </mxDefaultToolbar>
</mxEditor>
