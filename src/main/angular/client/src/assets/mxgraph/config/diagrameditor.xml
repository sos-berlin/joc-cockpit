<mxEditor defaultGroup="group" defaultEdge="connector">
  <include name="./assets/mxgraph/config/editor-commons.xml"/>
  <mxGraph as="graph" alternateEdgeStyle="verticalEdge" swimlaneNesting="0" dropEnabled="1">
    <add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
    <add as="isSwimlane"><![CDATA[
			function (cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'container');
			}
		]]></add>
    <mxStylesheet as="stylesheet">
      <add as="text">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="shadow" value="0"/>
        <add as="strokeColor" value="none"/>
        <add as="fillColor" value="transparent"/>
        <add as="gradientColor" value="none"/>
      </add>
      <add as="defaultVertex" extend="text">
        <add as="shape" value="rectangle"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="#333333"/>
      </add>
      <add as="defaultEdge">
        <add as="shape" value="connector"/>
        <add as="fontSize" value="10"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="center"/>
        <add as="rounded" value="1"/>
        <add as="labelBackgroundColor" value="#fff"/>
        <add as="strokeColor" value="gray"/>
        <add as="edgeStyle" value="elbowEdgeStyle"/>
        <add as="endArrow" value="classic"/>
      </add>
      <add as="verticalEdge">
				<add as="elbow" value="vertical"/>
			</add>
      <add as="ellipse">
        <add as="shape" value="ellipse"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="fillColor" value="transparent"/>
      </add>
    </mxStylesheet>
    <mxGraphModel as="model">
      <root>
        <mxCell id="0"/>
        <mxCell parent="0"/>
      </root>
    </mxGraphModel>
  </mxGraph>
  <mxDefaultToolbar as="toolbar">
    <add as="connect" mode="connect" icon="./assets/mxgraph/images/symbols/connect.gif"/>
<!--    <add as="show" action="show" icon="./assets/mxgraph/images/preview.png"/>
    <hr/>-->
    <add as="Job" template="Job" style="job" icon="./assets/mxgraph/images/job.png"/>
    <hr/>
    <add as="Fork" template="Fork" style="fork"
         icon="./assets/mxgraph/images/fork.png"/>
    <hr/>
    <add as="ForkList" template="ForkList" style="forkList"
         icon="./assets/mxgraph/images/fork-list.png"/>
    <hr/>
    <add as="Retry" template="Retry" style="retry" icon="./assets/mxgraph/images/retry.png"/>
    <hr/>
    <add as="Try" template="Try" style="try" icon="./assets/mxgraph/images/try.png"/>
    <hr/>
    <add as="If" template="If" style="if" icon="./assets/mxgraph/images/if.png"/>
    <hr/>
    <add as="Finish" template="Finish" style="finish" icon="./assets/mxgraph/images/finish.png"/>
    <hr/>
    <add as="Fail" template="Fail" style="fail" icon="./assets/mxgraph/images/fail.png"/>
    <hr/>
    <add as="Prompt" template="Prompt" style="prompt" icon="./assets/mxgraph/images/prompt.png"/>
    <hr/>
    <add as="PostNotice" template="PostNotice" style="postNotice" icon="./assets/mxgraph/images/publish.png"/>
    <hr/>
    <add as="ExpectNotice" template="ExpectNotice" style="expectNotice" icon="./assets/mxgraph/images/await.png"/>
    <hr/>
    <add as="Lock" template="Lock" style="lock" icon="./assets/mxgraph/images/lock.png"/>
    <hr/>
    <add as="AddOrder" template="AddOrder" style="addOrder" icon="./assets/mxgraph/images/addOrder.png"/>
    <hr/>
<!--    <add as="FileWatcher" template="FileWatcher" style="fileWatcher" icon="./assets/mxgraph/images/fileWatcher.png"/>
    <hr/>-->
    <add as="Paste" template="symbol" style="symbol" icon="./assets/mxgraph/images/paste.png"/>
  </mxDefaultToolbar>
</mxEditor>
