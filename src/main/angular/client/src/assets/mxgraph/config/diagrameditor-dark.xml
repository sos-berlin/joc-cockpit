<mxEditor defaultGroup="group" defaultEdge="connector">
  <ui>
    <add as="graph" element="graph"/>
    <add as="toolbar" element="toolbar"/>
  </ui>
  <Array as="templates">
    <add as="connector">
      <Connector label="" type="">
        <mxCell edge="1">
          <mxGeometry as="geometry" relative="1"/>
        </mxCell>
      </Connector>
    </add>
    <add as="Job">
      <Job jobName="job">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="180" height="40"/>
        </mxCell>
      </Job>
    </add>
    <add as="If">
      <If label="if" predicate="$returnCode > 0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </If>
    </add>
    <add as="AddOrder">
      <AddOrder label="addOrder">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </AddOrder>
    </add>
    <add as="Fork">
      <Fork label="fork">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fork>
    </add>
    <add as="ForkList">
      <ForkList label="forkList">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ForkList>
    </add>
    <add as="Lock" >
      <Lock label="lock">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Lock>
    </add>
    <add as="Cycle" >
      <Cycle label="cycle">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Cycle>
    </add>
    <add as="Retry">
      <Retry label="retry" maxTries="10" retryDelays="0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Retry>
    </add>
    <add as="Try">
      <Try label="try">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Try>
    </add>
    <add as="Fail">
      <Fail label="fail">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fail>
    </add>
    <add as="Finish">
      <Finish label="finish">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Finish>
    </add>
    <add as="ExpectNotices" >
      <ExpectNotices label="expectNotices">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ExpectNotices>
    </add>
    <add as="PostNotices">
      <PostNotices label="postNotices">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </PostNotices>
    </add>
    <add as="ConsumeNotices">
      <ConsumeNotices label="consumeNotices">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ConsumeNotices>
    </add>
    <add as="Prompt">
      <Prompt label="prompt">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Prompt>
    </add>
    <add as="FileWatcher">
      <FileWatcher label="fileWatcher" regex=".*">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="110" height="40"/>
        </mxCell>
      </FileWatcher>
    </add>
    <add as="symbol">
      <Symbol label="Instruction">
        <mxCell vertex="1" style="symbol;image=images/symbols/event.png">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Symbol>
    </add>
  </Array>
  <mxGraph as="graph" alternateEdgeStyle="verticalEdge" swimlaneNesting="0" dropEnabled="1">
    <add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
    <add as="isSwimlane"><![CDATA[
			function (cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'container');
			}
		]]></add>
    <mxStylesheet as="stylesheet">
      <add as="text">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="shadow" value="0"/>
        <add as="strokeColor" value="none"/>
        <add as="fillColor" value="transparent"/>
        <add as="gradientColor" value="none"/>
      </add>
      <add as="defaultVertex" extend="text">
        <add as="shape" value="rectangle"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="#fafafa"/>
        <add as="fontColor" value="#fafafa"/>
      </add>
      <add as="verticalEdge">
				<add as="elbow" value="vertical"/>
			</add>
      <add as="ellipse">
        <add as="shape" value="ellipse"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="fillColor" value="transparent"/>
      </add>
    </mxStylesheet>
    <mxGraphModel as="model">
      <root>
        <mxCell id="0"/>
        <mxCell parent="0"/>
      </root>
    </mxGraphModel>
  </mxGraph>
  <mxDefaultToolbar as="toolbar">
    <add as="connect" mode="connect" icon="./assets/mxgraph/images/symbols/connect.gif"/>
    <add as="Job" template="Job" style="job" icon="./assets/mxgraph/images/white-job.png"/>
    <hr/>
    <add as="Fork" template="Fork" style="fork"
         icon="./assets/mxgraph/images/white-fork.png"/>
    <hr/>
    <add as="ForkList" template="ForkList" style="forkList"
         icon="./assets/mxgraph/images/white-fork-list.png"/>
    <hr/>
    <add as="Retry" template="Retry" style="retry" icon="./assets/mxgraph/images/white-retry.png"/>
    <hr/>
    <add as="Try" template="Try" style="try" icon="./assets/mxgraph/images/white-try.png"/>
    <hr/>
    <add as="If" template="If" style="if" icon="./assets/mxgraph/images/white-if.png"/>
    <hr/>
    <add as="Cycle" template="Cycle" style="cycle" icon="./assets/mxgraph/images/white-cycle.png"/>
    <hr/>
    <add as="Finish" template="Finish" style="finish" icon="./assets/mxgraph/images/white-finish.png"/>
    <hr/>
    <add as="Fail" template="Fail" style="fail" icon="./assets/mxgraph/images/white-fail.png"/>
    <hr/>
    <add as="Prompt" template="Prompt" style="prompt" icon="./assets/mxgraph/images/white-prompt.png"/>
    <hr/>
    <add as="PostNotices" template="PostNotices" style="postNotices" icon="./assets/mxgraph/images/white-publish.png"/>
    <hr/>
    <add as="ConsumeNotices" template="ConsumeNotices" style="consumeNotices" icon="./assets/mxgraph/images/white-consume.png"/>
    <hr/>
    <add as="ExpectNotices" template="ExpectNotices" style="expectNotices" icon="./assets/mxgraph/images/white-await.png"/>
    <hr/>
    <add as="Lock" template="Lock" style="lock" icon="./assets/mxgraph/images/white-lock.png"/>
    <hr/>
    <add as="AddOrder" template="AddOrder" style="addOrder" icon="./assets/mxgraph/images/white-addOrder.png"/>
    <hr/>
 <!--   <add as="FileWatcher" template="FileWatcher" style="fileWatcher" icon="./assets/mxgraph/images/white-fileWatcher.png"/>
    <hr/>-->
    <add as="Paste" template="symbol" style="symbol" icon="./assets/mxgraph/images/white-paste.png"/>
  </mxDefaultToolbar>
</mxEditor>
