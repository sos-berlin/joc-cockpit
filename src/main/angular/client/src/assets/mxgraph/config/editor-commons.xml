<mxEditor>
  <ui>
    <add as="graph" element="graph"/>
    <add as="toolbar" element="toolbar"/>
  </ui>
  <Array as="templates">
    <add as="connector">
      <Connector label="" type="">
        <mxCell edge="1">
          <mxGeometry as="geometry" relative="1"/>
        </mxCell>
      </Connector>
    </add>
    <add as="Job">
      <Job jobName="job">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="180" height="40"/>
        </mxCell>
      </Job>
    </add>
    <add as="If">
      <If label="if" predicate="$returnCode > 0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </If>
    </add>
    <add as="AddOrder">
      <AddOrder label="addOrder">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="120" height="36"/>
        </mxCell>
      </AddOrder>
    </add>
    <add as="Fork">
      <Fork label="fork">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fork>
    </add>
    <add as="ForkList">
      <ForkList label="forkList">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ForkList>
    </add>
    <add as="Lock" >
      <Lock label="lock">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Lock>
    </add>
    <add as="Retry">
      <Retry label="retry" maxTries="10" retryDelays="0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Retry>
    </add>
    <add as="Try">
      <Try label="try">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Try>
    </add>
    <add as="Fail">
      <Fail label="fail">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fail>
    </add>
    <add as="Finish">
      <Finish label="finish">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Finish>
    </add>
    <add as="ExpectNotice" >
      <ExpectNotice label="expectNotice">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </ExpectNotice>
    </add>
    <add as="PostNotice">
      <PostNotice label="postNotice">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </PostNotice>
    </add>
    <add as="Prompt">
      <Prompt label="prompt">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Prompt>
    </add>
    <add as="FileWatcher">
      <FileWatcher label="fileWatcher" regex=".*">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="110" height="40"/>
        </mxCell>
      </FileWatcher>
    </add>
    <add as="symbol">
      <Symbol label="Instruction">
        <mxCell vertex="1" style="symbol;image=images/symbols/event.png">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Symbol>
    </add>
  </Array>
</mxEditor>
