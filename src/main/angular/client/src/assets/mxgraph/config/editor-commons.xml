<mxEditor>
  <ui>
    <add as="graph" element="graph"/>
    <add as="toolbar" element="toolbar"/>
  </ui>
  <Array as="templates">
    <add as="connector">
      <Connector label="" type="">
        <mxCell edge="1">
          <mxGeometry as="geometry" relative="1"/>
        </mxCell>
      </Connector>
    </add>
    <add as="Job">
      <Job jobName="job" label="" defaultArguments="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="180" height="40"/>
        </mxCell>
      </Job>
    </add>
    <add as="If">
      <If label="if" predicate="$returnCode > 0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </If>
    </add>
    <add as="Fork">
      <Fork label="fork">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fork>
    </add>
    <add as="Lock" >
      <Lock label="lock" lockId="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Lock>
    </add>
    <add as="Retry">
      <Retry label="retry" maxTries="10" retryDelays="0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Retry>
    </add>
    <add as="Try">
      <Try label="try">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Try>
    </add>
    <add as="Fail">
      <Fail label="fail" outcme="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Fail>
    </add>
    <add as="Finish">
      <Finish label="finish" outcme="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Finish>
    </add>
    <add as="Await" >
      <Await label="await" junctionPath="" timeout="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Await>
    </add>
    <add as="Publish">
      <Publish label="publish" junctionPath="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Publish>
    </add>
    <add as="FileWatcher">
      <FileWatcher label="fileWatcher" agent="" regex=".*" directory="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="110" height="40"/>
        </mxCell>
      </FileWatcher>
    </add>
    <add as="symbol">
      <Symbol label="Instruction">
        <mxCell vertex="1" style="symbol;image=images/symbols/event.png">
          <mxGeometry as="geometry" width="68" height="68"/>
        </mxCell>
      </Symbol>
    </add>
  </Array>
</mxEditor>
